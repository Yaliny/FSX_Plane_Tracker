<Window x:Class="FSX_Plane_Tracker.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FSX_Plane_Tracker"
        mc:Ignorable="d"
        xmlns:m="clr-namespace:Microsoft.Maps.MapControl.WPF;assembly=Microsoft.Maps.MapControl.WPF"
        xmlns:db="clr-namespace:Codeplex.Dashboarding;assembly=Codeplex.Dashboarding"
        Title="MainWindow" Height="550" Width="700" Closing="Window_Closing">
    <Grid Height="500">
        <Grid Height="250" Width="680" VerticalAlignment="Top">
            <m:Map Name="viewmap" CredentialsProvider="AkIkAsKzzXkSWT54vfugNilSzxbrvckDvprWj-ZdpQ-WjbabgA-Rx75wJJ1Je8sf" Mode="AerialWithLabels" Margin="10,10,10,11">
            </m:Map>
        </Grid>
        <Grid Height="250" Width="680" VerticalAlignment="Bottom">
            <db:Dial360 x:Name="airSpeed" Minimum="0" Maximum="700" Value="0" Margin="10,10,499,70" FontSize="11" ValueTextColor="#FF00DCFF" ValueTextFormat="{}{0:0} knots" />
            <TextBlock x:Name="debug" HorizontalAlignment="Left" Margin="279,15,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="182" Width="346"><Run Text="TextBlock"/></TextBlock>
        </Grid>
    </Grid>
</Window>
